					},
					{
						"propagate": false,
						"group": "Summary",
						"name": "IntStart",
						"descriptor-type": "custom",
						"descriptor-code": "",
						"value": "\"Int start date\"~formatdate(am(\"IntFirst\", \"Date\"))"
					},
